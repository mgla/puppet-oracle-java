# notes for WIP module

wget --header "Cookie: oraclelicense=accept-securebackup-cookie" http://download.oracle.com/otn-pub/java/jdk/8u91-b14/server-jre-8u91-linux-x64.tar.gz -O /tmp/java.tar.gz
mkdir /opt/jre
tar vxf /tmp/java.tar.gz -C /opt/jre
update-alternatives --install /usr/bin/java java /opt/jre/jdk1.8.0_91/bin/java 100
update-alternatives --install /usr/bin/javac javac /opt/jre/jdk1.8.0_91/bin/javac 100
